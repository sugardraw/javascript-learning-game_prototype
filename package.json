{
  "name": "webpack-reloaded",
  "version": "1.0.0",
  "description": "``` npm init ```",
  "main": "index.js",
  "scripts": {
    "start": "react-scripts start",
    "watch": "npm run dev -- --watch",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "dev-live": "webpack --mode development ./src/js/main.js --output ./public/js/main.js | webpack-dev-server --progress --hot --inline --mode development ./src/js/main.js --output ./dist/js/main.js",
    "dev": "webpack --mode development ./src/js/main.js --output ./public/js/main.js",
    "production": "webpack --mode production ./src/js/main.js --output ./public/js/main.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sugardraw/webpack-reloaded.git"
  },
